  CXXFLAGS  += $(CFLAGS) 
  LDFLAGS   += -s -fopenmp -flto=auto
  LIBS      += -lGLEW -lSDL2 -lSDL2_image -lGL
  RESFLAGS  += $(DEFINES) $(INCLUDES) 
  LDDEPS    += 
